{% extends 'layout.twig' %}

{% block body %}
				{% if error != '' %}
				<div class="row">
						<div class="col-md-12">Ocorreu um erro na execução</div>
				</div>
				{% endif %}
				<div class="row">
					<div class="form-group">
						<div class="col-md-4">
							<a href="/contato">
								<button class="btn btn-default form-control">
									Contato
								</button>
							</a>
						</div>
						<div class="col-md-4">
							<a href="/ramais">
								<button type="button" class="btn btn-default form-control">
									<span>Ramais</span>
								</button>
							</a>
						</div>
						<div class="col-md-4">
							<a href="/consulta">
								<button type="button" class="btn btn-default form-control">
									<span> Consultar Ramais</span>
								</button>
							</a>
						</div>
						<div class="col-md-2">
							<div class="form-group">
								<a href="/cadastro">
									<input name="cadastre-se" type="button" value="cadastre-se" class="form-control botao"/>
								</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
{% endblock %}
